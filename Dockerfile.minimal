# Minimal Dockerfile for testing
FROM python:3.10-slim

WORKDIR /app

# Copy requirements first
COPY requirements-minimal.txt requirements.txt

# Install minimal dependencies
RUN pip install --no-cache-dir -r requirements.txt

# Copy app files
COPY . .

# Expose port
EXPOSE 5000

